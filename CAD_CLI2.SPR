*       *                                                         
*       * Function Origin:                                        
*       *                                                         
*       * From Platform:       Windows                            
*       * From Screen:         CAD_CLI2,     Record Number:   48  
*       * Variable:            wnome                              
*       * Called By:           VALID Clause                       
*       * Object Type:         Field                              
*       * Snippet Number:      1                                  
*       *                                                         
*       *********************************************************
*
FUNCTION _3ru0jcwwb     &&  wnome VALID
#REGION 1
wnome = schonetekst(wnome)
SHOW GETS

IF inclui AND !ISBLANK(wnome)
	SELECT cliente
	DO studentzoeken
	IF RECCOUNT('als') > 0
		SELECT als
		SELECT cliente.nome + ' ' + ;
			STR(als.percentage,9,2) + '%   ' + ;
			ALLTRIM(cliente.end) + ' ' + ;
			ALLTRIM(cliente.cidade) + ' ' + ;
			ALLTRIM(cliente.uf), ;
			cliente.codigo ;
			FROM cliente, als ;
			WHERE cliente.codigo == als.id ;
			ORDER BY cliente.nome ;
			INTO ARRAY lijstals
		alss = 1
		ingevoerdegegevens = wnome
		gelijkheader = 'NOME' + ;
			SPACE(40) + ;
			'% SEMELHANÇA   ' + ;
			'DADOS ADICIONAIS'
		gelijkfooter = 'QUANTIDADE DE REGISTRO(S) ENCONTRADO(S) - ' + ;
			ALLTRIM(STR(ALEN(lijstals)/2))
		terugkeren = .F.
		DO gelijk.spr
		IF terugkeren
			DO studentGevonden
		ENDIF
	ENDIF
ENDIF
SHOW GETS

PROCEDURE studentzoeken
	CREATE CURSOR als (id N(6), ;
		percentage N(6,2) )
	SELE als
	INDEX ON percentage DESC TAG percentage

	i = 1
	SET EXACT OFF
	SELECT 0
	USE &dircont\cliente AGAIN ORDER TAG NOME ALIAS klantnaam
	SELE klantnaam
	recordbedrag = 0
	CALC CNT() TO recordbedrag FOR klantnaam.nome = LEFT(wnome, 1)
	SCAN FOR NOME = LEFT(wnome, 1)
		WAIT WIND 'Buscando Semelhanças...' + ALLTRIM(STR(i,10)) + ;
			' de ' + ALLTRIM(STR(recordbedrag,10)) NOWAIT
		i = i + 1
		lokpercentage = levpercent( ALLTRIM(klantnaam.nome), ALLTRIM(wnome))
		IF  lokpercentage > 85
			SELE als
			APPEND BLANK
			REPLACE als.id WITH klantnaam.codigo
			REPLACE als.percentage WITH lokpercentage
		ENDIF
		SELE klantnaam
	ENDSCAN

	SELE klantnaam
	USE
	RETURN

PROCEDURE studentGevonden
	SELECT cliente
	SEEK (lijstals(alss,2))
	DO consulta
	SHOW GET bt_adcurso, 1 ENABLE
	SHOW GET bt_exccurs, 1 ENABLE
	SHOW GET clicurs ENABLE
	SHOW GET mudanome ENABLE
	IF wsetor <> "FIN"
		editnome = .F.
	ENDIF
	DO clicur_l
	cli_ent_al = "ALTERAÇÃO DE ALUNO"
	SHOW GET mencli2, 1 PROMPT "Altera"
	_CUROBJ = OBJNUM(wend)
RETURN

