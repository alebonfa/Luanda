#REGION 1
PROCEDURE imp_ficha
	afdVerko = zoekContact(wcodigo)
	SELECT cliente
	numrec = RECNO()
	IF vis_impr
		REPORT FORMAT clicompl TO PRINTER PREVIEW RECORD numrec
	ELSE
		REPORT FORMAT clicompl TO PRINTER PROMPT NOCONSOLE RECORD numrec
	ENDIF
	RETURN
	*--

