	ENDIF
CASE mencli2 = 2
	access = veracess(wcoduser,1,"ALT")
	IF access
		DO Clicurso.SPR
	ENDIF
CASE mencli2 = 3
	access = veracess(wcoduser,1,"IMP")
	IF access
		DO imp_ficha
	ENDIF
CASE mencli2 = 4
	access = veracess(wcoduser,1,"IMP")
	IF access
		DO cli_pres
	ENDIF
CASE mencli2 = 5
	DO sta_list
	stas = 1
CASE mencli2 = 6
	access = veracess(wcoduser,32,"INC")
	IF access
		tcc_acad = .F.
		DO Lanc_tcc.SPR
	ENDIF
CASE mencli2 = 7
	DO zera_var
