PROCEDURE exc_curso
	CONF = winconf("ATENÇÃO", ;
		"Tem certeza que deseja excluir este curso?")
	IF CONF
		SELECT clicurso
		SEEK l_clicur(clicurs,2)
		DELETE
		DO clicur_l
		SHOW GETS
	ENDIF
	RETURN
	*--

	*--
PROCEDURE alt_situac
PARAMETER psit
	IF !ISBLANK(wclidata)
		CONF = winconf("ATENÇÃO","Alterar status para: " + ALLTRIM(psit) + "?")
		IF conf
			SELECT clicurso
			SEEK l_clicur(clicurs,2)
			REPLACE clicurso.situacao WITH psit
			REPLACE clicurso.data WITH wclidata
			DO clicur_l
			SHOW GETS
		ENDIF
	ELSE
		DO WINALERT WITH 'ATENÇÃO','ENTRE COM UMA DATA DE ALTERAÇÃO DE STATUS'
	ENDIF
	RETURN
