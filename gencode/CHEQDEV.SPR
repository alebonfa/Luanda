FUNCTION _2VO0YOT89     &&  menu2 VALID
#REGION 1
DO CASE
 CASE menu2 = 1
  access = veracess(wcoduser,21,"EXC")
  IF access
   conf = winconf("ATENÇÃO", ;
          "TEM CERTEZA QUE DESEJA EXCLUIR ESTE CHEQUE?")
   IF conf
    SELECT cheques
    SEEK list_cheq(cheqs,2)
    DELETE
   ENDIF
   DO filtra
  ENDIF
 CASE menu2 = 2
  access = veracess(wcoduser,21,"IMP")
  IF access
   DO imp_nome
  ENDIF
 CASE menu2 = 3
  access = veracess(wcoduser,21,"IMP")
  IF access
   DO imp_data
  ENDIF
 CASE menu2 = 4
  access = veracess(wcoduser,21,"ALT")
  IF access
   IF wtodos
    conf = winconf("ATENÇÃO", ;
           "REGISTRAR ESTA OBSERVAÇÃO EM TODOS OS CHEQUES?")
    IF conf
     SELECT cheques
     FOR i = 1 TO ALEN(list_cheq) / 2
      SEEK list_cheq(i,2)
      REPLACE cheques.hist WITH whist
     ENDFOR
     DO filtra
    ENDIF
   ELSE
    SELECT cheques
    SEEK list_cheq(cheqs,2)
    REPLACE cheques.hist WITH whist
    DO filtra
   ENDIF
  ENDIF
 CASE menu2 = 5
  access = veracess(wcoduser,21,"INC")
  IF access
   DO dup_cheque
  ENDIF
 CASE menu2 = 6
  CLEAR READ
ENDCASE
