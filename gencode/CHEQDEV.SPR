*       *********************************************************
*       *                                                         
*       * _2VO0YOT82           menu VALID                         
*       *                                                         
*       * Function Origin:                                        
*       *                                                         
*       * From Platform:       Windows                            
*       * From Screen:         CHEQDEV,     Record Number:   62   
*       * Variable:            menu                               
*       * Called By:           VALID Clause                       
*       * Object Type:         Push Button                        
*       * Snippet Number:      4                                  
*       *                                                         
*       *********************************************************
*
FUNCTION _2VO0YOT82     &&  menu VALID
#REGION 1
access = veracess(wcoduser,21,"ALT")
IF access
 IF !wtodos
  conf = winconf("ATENÇÃO", ;
         "TEM CERTEZA QUE DESEJA EXECUTAR ESTA TRANSFERÊNCIA?")
  IF conf
   SELECT cheques
   DO CASE
    CASE menu = 1
     SEEK (list_cheq(cheqs,2))
     REPLACE situacao WITH "CT"
    CASE menu = 2
     SELECT cheques
     SEEK (list_cheq(cheqs,2))
     REPLACE situacao WITH "DP"
     SELECT extrato
     APPEND BLANK
     REPLACE extrato.data WITH wdatatr
     IF cheques.pre
      REPLACE extrato.historico WITH "DEPÓSITO - Cheque-Pré"
     ELSE
      REPLACE extrato.historico WITH "DEPÓSITO - Cheque/Dia"
     ENDIF
     REPLACE extrato.valor WITH cheques.valor
     REPLACE extrato.debito WITH "+"
     REPLACE extrato.destino WITH list_ban(bans,2)
     REPLACE extrato.numerolanc WITH 999999
     REPLACE extrato.dinheiro WITH "DN"
     SELECT cheques
    CASE menu = 3
     SEEK (list_cheq(cheqs,2))
     REPLACE situacao WITH "PG"
    CASE menu = 4
     SEEK (list_cheq(cheqs,2))
     REPLACE situacao WITH "CS"
    CASE menu = 5
     SEEK (list_cheq(cheqs,2))
     REPLACE situacao WITH "DS"
    CASE menu = 6
     SEEK (list_cheq(cheqs,2))
     REPLACE situacao WITH "DV"
    CASE menu = 7
     SEEK (list_cheq(cheqs,2))
     REPLACE situacao WITH "NE"
   ENDCASE
   REPLACE data_tr WITH wdatatr
   DO filtra
  ELSE
   WAIT WINDOW "TRANSFERÊNCIA CANCELADA"
  ENDIF
 ELSE
  conf = winconf("ATENÇÃO", ;
         "REALMENTE DESEJA TRANSFERIR DE TODOS ESTES CHEQUES?")
  IF conf
   SELECT cheques
   DO CASE
    CASE menu = 1
     aux = "CT"
    CASE menu = 2
     aux = "DP"
    CASE menu = 3
     aux = "PG"
    CASE menu = 4
     aux = "CS"
    CASE menu = 5
     aux = "DS"
    CASE menu = 6
     aux = "DV"
    CASE menu = 7
     aux = "NE"
   ENDCASE
   SELECT cheques
   SEEK (list_cheq(1,2))
   auxpre = cheques.pre
   FOR i = 1 TO ALEN(list_cheq) / 2
    SEEK list_cheq(i,2)
    REPLACE situacao WITH aux
    REPLACE data_tr WITH wdatatr
   ENDFOR
   IF aux = "DP"
    SELECT extrato
    APPEND BLANK
    REPLACE extrato.data WITH wdatatr
    IF auxpre
     REPLACE extrato.historico WITH "DEPÓSITO - Cheque-Pré"
    ELSE
     REPLACE extrato.historico WITH "DEPÓSITO - Cheque/Dia"
    ENDIF
    REPLACE extrato.valor WITH totche
    REPLACE extrato.debito WITH "+"
    REPLACE extrato.destino WITH list_ban(bans,2)
    REPLACE extrato.numerolanc WITH 999999
    REPLACE extrato.dinheiro WITH "DN"
   ENDIF
   DO filtra
  ELSE
   WAIT WINDOW "TRANSFERÊNCIA CANCELADA"
  ENDIF
 ENDIF
 SHOW GETS
ENDIF
*--

