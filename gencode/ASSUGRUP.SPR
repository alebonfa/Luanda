#REGION 1
PROCEDURE mudancas
IF !ISBLANK(wdescricao)
 SELECT crm_assu
 IF inclui
  CALCULATE MAX(crm_assu.codigo) TO ult_cod
  APPEND BLANK
  REPLACE crm_assu.codigo WITH (ult_cod + 1)
 ENDIF
 REPLACE crm_assu.descricao WITH wdescricao
 DO zera_var
ENDIF
RETURN
