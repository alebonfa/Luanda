PROCEDURE mudancas
SELECT tip_paga
IF inclui
 APPEND BLANK
ENDIF
REPLACE tip_paga.codigo WITH wcodigo
REPLACE tip_paga.descricao WITH wdescricao
REPLACE tip_paga.juros WITH wjuros
DO zera_var
RETURN
