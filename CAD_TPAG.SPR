PROCEDURE consulta
SELECT tip_paga
wcodigo = tip_paga.codigo
wdescricao = tip_paga.descricao
wjuros = tip_paga.juros
inclui = .F.
SHOW GET menu, 1 PROMPT "Altera"
RETURN
