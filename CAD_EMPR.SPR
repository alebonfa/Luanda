#REGION 1
PROCEDURE emp_list
DIMENSION list_emp[ 1]
list_emp[ 1] = "Não existem Empresas cadastradas..."
SELECT empresas.nome, empresas.id FROM Empresas ORDER BY  ;
       empresas.nome INTO ARRAY list_emp
emps = 1
RETURN
*--

*--
PROCEDURE mudancas
IF !ISBLANK(wnome)
 SELECT empresas
 IF inclui
  ult_id = 0
  GOTO BOTTOM
  ult_id = empresas.id
  APPEND BLANK
  REPLACE empresas.id WITH ult_id + 1
 ENDIF
 SELECT empresas
 REPLACE empresas.cnpj WITH wcnpj
 REPLACE empresas.nome WITH wnome
 REPLACE empresas.endereco WITH wendereco
 DO emp_list
 DO zera_var
ELSE
 DO winalert WITH "ATENÇÃO",  ;
    "DADOS INSUFICIENTES PARA O CADASTRO!"
ENDIF
RETURN
*--

*--
PROCEDURE consulta
SELECT empresas
wnome = empresas.nome
wcnpj = empresas.cnpj
wendereco = empresas.endereco
_CUROBJ = OBJNUM(wcnpj)
inclui = .F.
SHOW GET menu, 1 PROMPT "Altera"
RETURN
*--

*--
PROCEDURE zera_var
wnome = ""
wcnpj = ""
wendereco = ""
inclui = .T.
SHOW GET menu, 1 PROMPT "Adiciona"
RETURN
