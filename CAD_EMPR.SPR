*       * Object Type:         Push Button                        
*       * Snippet Number:      2                                  
*       *                                                         
*       *********************************************************
*
FUNCTION _2VO0YOQYQ     &&  menu VALID
#REGION 1
DO CASE
 CASE menu = 1
  IF inclui
   access = veracess(wcoduser,13,"INC")
  ELSE
   access = veracess(wcoduser,13,"ALT")
  ENDIF
  IF access
   DO mudancas
  ENDIF
 CASE menu = 2
  WAIT WINDOW TIMEOUT 1 "ALTERAÇÕES CANCELADAS"
  DO zera_var
 CASE menu = 3
  access = veracess(wcoduser,13,"EXC")
  IF access
   SELECT modulos
   ncur = 0
   CALCULATE FOR modulos.empr_id == list_emp(emps,2) CNT() TO  ;
             ncur
   IF ncur = 0
    conf = winconf("ATENÇÃO", ;
           "Tem certeza que deseja excluir este curso?")
    IF conf
     SELECT empresas
     SEEK list_emp(emps,2)
     DELETE
     DO emp_list
     emps = 1
    ENDIF
   ELSE
    DO winalert WITH "ATENÇÃO",  ;
       "NÃO É POSSÍVEL EXCLUIR ESTA EMPRESA."
   ENDIF
  ENDIF
 CASE menu = 4
  CLEAR READ
ENDCASE
_CUROBJ = OBJNUM(wcnpj)
SHOW GETS
